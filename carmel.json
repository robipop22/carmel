{
  "name": "Carmel",
  "template": "default",
  "env": "production",
  "id": "io.carmel",
  "androidSdkDir": "~/Library/Android/sdk",
  "app": {
    "start": {
      "stack": ["intro", "download"]
    }
  },
  "transitions": ["replace://start"],
  "provisioning": {
    "rest": {
      "url": "https://api.carmel.io"
    }
  },
  "build": {
    "platforms": ["darwin/x64"]
  },
  "theme": {
    "logoImage": "logo-white.png",
    "logoLightImage": "logo.png",
    "headerColor": "#FF5722",
    "textColor": "#546E7A",
    "linkColor": "#0288D1",
    "linkHoverColor": "#64B5F6",
    "linkHoverBackgroundColor": "#F5F5F5",
    "progressColor": "rgba(50,50,50,0.9)",
    "primaryColor": "#00bcd4",
    "secondaryColor": "#00BFA5",
    "statusBarLight": false,
    "navigationColor": "#FFFFFF",
    "navigationTintColor": "#37474F",
    "navigationTextButton": "#FFFFFF",
    "backgroundColor": "#999999",
    "footerTintColor": "#CFD8DC",
    "footerHeaderColor": "#90A4AE",
    "footerColor": "#546E7A",
    "footerBottomColor": "#37474F",
    "footerSocialIconsColor": "#CFD8DC",
    "footerSocialIconsColorHover": "#0097A7",
    "notFoundPageText": "local://404"
  },
  "styles": {
    "web": [
      "https://fonts.googleapis.com/css?family=Roboto",
      "https://fonts.googleapis.com/css?family=Montserrat:400,700",
      "https://fonts.googleapis.com/icon?family=Material+Icons",
      "https://unpkg.com/material-components-web@5.1.0/dist/material-components-web.min.css",
      "https://unpkg.com/antd@4.0.2/dist/antd.min.css"
    ]
  },
  "scripts": {
    "web": ["https://unpkg.com/antd@4.0.2/dist/antd.js"]
  },
  "info": {
    "copyright": "Â© 2020",
    "title": "Carmel",
    "summary": "The Decentralized Tech Marketplace",
    "watermark": "Created with Carmel.",
    "cover": "busy.r.png",
    "analytics": {
      "type": "google",
      "key": "UA-99031266-1"
    }
  }
}
